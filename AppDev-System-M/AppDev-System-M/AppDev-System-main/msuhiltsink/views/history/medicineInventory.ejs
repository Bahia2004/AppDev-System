<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
</head>
<body>
  <h1>Medicine History</h1>
  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Item Name</th>
        <th>Category</th>
        <th>Quantity</th>
        <th>Expiry Date</th>
        <th>Created At</th>
      </tr>
    </thead>
    <tbody>
      <% inventory.forEach(item => { %>
        <tr>
          <td><%= item.inventory_id %></td>
          <td><%= item.item_name %></td>
          <td><%= item.category %></td>
          <td><%= item.quantity %></td>
          <td><%= item.expiry_date %></td>
          <td><%= item.created_at %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <a href="/history">Back to History</a>
</body>
</html>